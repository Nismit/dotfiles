  email = 22581499+Nismit@users.noreply.github.com

[core]
  excludesfile = ~/.gitignore_global

[alias]
  cm = commit -m
  co = checkout
  one = log --pretty=oneline
  tree = log --graph --oneline --decorate
  rbp = fetch --prune

[color "diff"]
  meta = 242 238
  frag = 239 236
  old = 167 normal
  new = 030 normal
  context = 240
  commit = 246 024

[diff]
  compactionHeuristic = true

# [pager]
#   log = diff-highlight | less
#   show = diff-highlight | less
#   diff = diff-highlight | less
[credential]
  helper = osxkeychain
